{
  "name": "dwm-frontend",
  "version": "1.0.0",
  "description": "dwm-frontend",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "dev-next": "env-cmd ./.config/dev.env next",
    "dev-start": "env-cmd ./.config/dev.env next start",
    "prod-next": "env-cmd ./.config/prod.env next",
    "prod-start": "env-cmd ./.config/prod.env next start",
    "build": "next build",
    "jest": "",
    "jest-ci": "",
    "lint": "eslint . --ext=js,jsx",
    "lint:fix": "eslint . --ext=js,jsx --fix"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "@types/googlemaps": "^3.30.16",
    "@types/markerclustererplus": "^2.1.33",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "animate.css": "^3.7.0",
    "apollo-boost": "^0.1.22",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "graphql": "^14.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "next-images": "^1.0.3",
    "nouislider": "^12.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-animate-on-scroll": "^2.1.5",
    "react-apollo": "^2.3.2",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.6.1",
    "react-google-maps": "^9.4.5",
    "react-image-gallery": "^0.8.12",
    "react-jss": "^8.6.1",
    "react-router-dom": "^4.3.1",
    "react-slick": "^0.23.2",
    "react-swipeable-views": "^0.13.0",
    "react-tagsinput": "^3.19.0",
    "tachyons": "^4.11.1",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-css-in-js": "^1.5.1",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.7.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "prettier": "^1.15.1",
    "pretty-quick": "^1.8.0"
  },
  "prettier": {
    "eslintIntegration": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "semi": false,
    "arrowParens": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "css-in-js"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "settings": {
      "react": {
        "version": "^16.7.0-alpha.2"
      }
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0
    }
  }
}
